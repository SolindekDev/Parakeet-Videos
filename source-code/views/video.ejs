                        <% } %>
                    </div>
                </div>
                    
            </div>
            <div class="right"></div>
            <div class="clear"></div>
        </div>
        </div>
        <footer class="container-fluid">
            <div class="row">
                <div class="col-xxl">
                    <h1>Parakeet</h1>
                    <span style="padding-bottom: 10px !important;">Upload videos and have fun</span><br>
                </div>
                <div class="col">
                    <h1>Policy</h1>
                    <a href="/policy-private">Policy private</a><br>
                    <a href="/rules">Rules</a><br>
                    <a href="/thanks">Thanks</a>
                </div>
                <div class="col">
                    <h1>Resources</h1>
                    <a href="/developers">Developers portal</a><br>
                    <a href="/api">API</a><br>
                    <a href="/report">Report</a><br>
                </div>
                <div class="col">
                    <h1>Us</h1>
                    <a href="/about">About</a><br>
